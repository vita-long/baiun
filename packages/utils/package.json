  "scripts": {
    "clean": "rm -rf dist",
    "build:js": "tsup",
    "build:dts": "tsc --emitDeclarationOnly",
    "build": "pnpm run clean && pnpm run build:js && pnpm run build:dts",
    "dev": "tsup --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  },
  "dependencies": {
    "axios": "^1.13.2"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ]
